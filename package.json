{
  "name": "interactive-twitch-chat",
  "version": "0.0.3",
  "description": "FreddiÂ´s Interactive Twitch Chat",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "publish": null,
  "keywords": [],
  "author": "Freddi <mail@FrederikHeinrich.de>",
  "license": "ISC",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "interactive-twitch-chat",
            "setupIcon": "src/icon.ico"
            
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "frederikheinrich",
              "name": "Interactive-Twitch-Chat"
            },
            "draft": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.6",
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "electron": "17.1.0"
  },
  "build": {
    "extraResources": [
      "./public/**"
    ],
    "fileAssociations": {
      "icon": "src/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrederikHeinrich/Interactive-Twitch-Chat.git"
  },
  "bugs": {
    "url": "https://github.com/FrederikHeinrich/Interactive-Twitch-Chat/issues"
  },
  "homepage": "https://github.com/FrederikHeinrich/Interactive-Twitch-Chat#readme"
}
